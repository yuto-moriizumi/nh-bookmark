{
  "manifest_version": 3,
  "name": "nh-bookmark",
  "version": "1.5.0",
  "description": "",
  "author": "Y式植林装置",
  "icons": { "32": "logo32.png", "48": "logo48.png", "128": "logo.png" },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["browser-polyfill.js"]
    }
  ],
  "permissions": [
    "activeTab",
    "scripting",
    "menus",
    "contextMenus",
    "tabs",
    "notifications"
  ],
  "action": { "default_popup": "popup.html" },
  "host_permissions": [
    "https://paratranz.cn/projects/*",
    "https://1thrt62esf.execute-api.ap-northeast-1.amazonaws.com/*"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "Ysiki.Shokurin@gmail.com",
      "strict_min_version": "109.0"
    }
  }
}
